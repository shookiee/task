<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">
	<!-- 특정 사용자 조회 -->
	<select id="getUser" parameterType="String" resultType="userVo">
		SELECT *
		FROM BOARD_USERS
		WHERE USERID = #{userId}
	</select>
</mapper>